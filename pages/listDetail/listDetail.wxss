@keyframes fadeIn {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

.image_layer {
  width: 100%;
  height: 100%;
  background: #000;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 2;
  overflow: scroll;
  padding: 0 10rpx;
  box-sizing: border-box;
  text-align: center;
  animation: fadeIn 0.3s;
  padding-bottom: 56rpx;
}

.image_layer image {
  display: block;
  width: 100%;
  margin: 0 auto;
}

.image_layer .btn_box {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 80rpx;
  display: flex;
}

.image_layer .btn_box view {
  flex: 1;
  text-align: right;
  margin: 0 100rpx;
  margin-top: 6rpx;
}

.image_layer .btn_box .close {
  text-align: left;
}

.image_layer view button {
  width: 44rpx;
  height: 44rpx;
  display: inline-block;
  background: url(http://bmob-cdn-7744.b0.upaiyun.com/2016/12/23/3ce6a3f74067786080a5943f750a3ce3.png) no-repeat center center;
  background-size: 100% auto;
}

.image_layer .btn_box .close button {
  background-image: url(http://bmob-cdn-7744.b0.upaiyun.com/2016/12/23/5790e798401ac8de8003175b01dc359c.png);
}

.image_layer .btn_box .close button::after {
  border: 0 none;
}

