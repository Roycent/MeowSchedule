.main{
    background:#EFEFF4;
    height: 100%;
    width: 100%;
    margin-top: 0;
    margin-bottom: 0;
    padding-top: 0rpx;
}
.MyView{
     width: 75px;
     height: 75px; 
     background-position: 50% 50%; 
     background-size: cover; 
     border-radius: 48px; 
     margin-bottom: 5px; 
     margin-top: 24%;
     margin-left: 40%;
     margin-right: 45%;
}

.MyIcon{
    position: center;
    width: 100%; 
    height: 100%; 
    border-radius: 48px; 
    border-bottom-right-radius: 48px; 
    border-bottom-left-radius: 48px; 
    border-top-right-radius: 48px; 
    border-top-left-radius: 48px; 
}
.MoreInfo{
    margin-top: 10%;
}
.NavList1{
    padding-top: 10px; 
    padding-bottom: 10px; 
    padding-right: 15px; 
    display: flex; 
    justify-content: space-between; 
    align-items: center;  
    border-bottom: 1px solid rgb(229, 229, 229); 
    background: white;
}

.NavList2{
    padding-top: 10px; 
    padding-bottom: 10px; 
    padding-right: 15px; 
